
    <div class="form-group">
        <label class="col-md-3 control-label">หน้าใหม่</label>
        <div class="col-md-5">
            <select style="max-width:150px;" class="form-control" id="NewPage" name="NewPage">
                <option value="1">ทุกคำถาม</option>
                @{
                    for (var i = 2; i < 11; i++)
                    {
                        <option value="@i">ทุก @i คำถาม</option>
                    }
                }
            </select>
        </div>
    </div>

    <div class="form-group">
        <label class="col-md-3 control-label">วิธีการเปลี่ยนหน้า</label>
        <div class="col-md-5">
            <select style="max-width:150px;" class="form-control" id="HowtoPage" name="HowtoPage">
                <option value="0">อิสระ</option>
                <option value="1">ห้ามย้อนกลับ</option>
            </select>
        </div>
    </div>



